<h1> Sign in </h1>
<% if flash[:error] %>
  <%= flash[:error] %>
<% end %>

<%= form_for :user, url: sessions_path do |f| %>
<p>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
</p>
<p>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
</p>
<p>
    <%= f.submit "Sign in" %>
</p>
<% end %>
